{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "loader.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/post.html",
      "dest": "/post.html"
    },
    {
      "src": "/posts.json",
      "dest": "/posts.json"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "buildCommand": "npm install && npm run build"
} 